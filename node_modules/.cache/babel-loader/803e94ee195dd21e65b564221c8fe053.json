-firebase\",\n    storageBucket: \"kram-music-firebase.appspot.com\",\n    messagingSenderId: \"267391010196\",\n    appId: \"1:267391010196:web:a2a43d54b3803d841c7b81\",\n    databaseURL: \"https://kram-music-firebase-default-rtdb.europe-west1.firebasedatabase.app/\",\n  };\n  initializeApp(firebaseConfig);\n\ncomponents.forEach(component => {\n    app.component(component.name, component);    \n});\n\nconst auth = getAuth();\nonAuthStateChanged(auth, (user) => {\n  if (user) {\n    store.commit('true_sign_in')\n  } else {\n    store.commit('false_sign_in')\n  }\n});\n  const db = getDatabase();\n  const post_load = ref(db, 'posts/');\n  onValue(post_load, (snapshot) => {\n    store.state.posts.post_list = snapshot.val();\n    store.state.posts.post_list.splice(0,1)\n    store.state.posts.post_list.forEach(element => console.log(typeof(element.image)))\n    store.state.posts.post_list.forEach(element => element['add_like']=false)\n    store.state.posts.post_list.forEach(element => element['add_dislike']=false)\n    console.log (store.state.posts.post_list)\n  });\n\napp.use(router)\napp.use(store)\napp.mount('#app');"]}]}